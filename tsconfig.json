{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "checkJs": false,
    "jsx": "react-jsx",
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
  "types": ["node", "@playwright/test"]
  },
  "include": ["src", "playwright.config.ts", "tests", "**/*.astro"],
  "exclude": ["node_modules", "dist"]
}
